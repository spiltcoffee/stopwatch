{
  "name": "vr-stopwatch",
  "productName": "VR Stopwatch",
  "version": "1.1.0",
  "description": "Timer designed specifically for helping run VR sessions",
  "license": "MIT",
  "type": "commonjs",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "web": "webpack --config webpack.web.config.ts --progress",
    "web:dev": "webpack-dev-server --config webpack.web.config.ts --progress --mode development",
    "lint": "eslint --ignore-path .gitignore \"**/*.{js,vue}\" --fix",
    "format": "prettier --ignore-path .gitignore \"**/*.{js,json,ts,yml,md,html,vue}\" --write --list-different",
    "postinstall": "yarn husky install"
  },
  "keywords": [
    "vr",
    "stopwatch",
    "electron"
  ],
  "author": {
    "name": "SpiltCoffee",
    "email": "spiltcoffee@users.noreply.github.com"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^21.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "fs-jetpack": "^5.1.0",
    "lodash.get": "^4.4.2",
    "moment": "^2.29.4",
    "process": "^0.11.10",
    "update-electron-app": "^2.0.1",
    "uuid": "^9.0.1",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.4.2",
    "@electron-forge/maker-squirrel": "6.4.2",
    "@electron-forge/plugin-auto-unpack-natives": "^6.4.2",
    "@electron-forge/plugin-electronegativity": "^6.4.2",
    "@electron-forge/plugin-webpack": "6.4.2",
    "@electron-forge/publisher-github": "6.4.2",
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "autoprefixer": "^10.4.16",
    "css-loader": "^6.8.1",
    "cssnano": "^6.0.1",
    "electron": "27.0.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-vue": "^9.17.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "node-loader": "^2.0.0",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.3",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.3",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "vue-eslint-parser": "^9.3.2",
    "vue-loader": "^17.3.0",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  },
  "lint-staged": {
    "*.{json,yml,md,html,css}": [
      "prettier --ignore-path .gitignore --write"
    ],
    "*.{js,vue}": [
      "eslint --ignore-path .gitignore",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@3.6.3"
}
